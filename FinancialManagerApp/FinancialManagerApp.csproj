<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\logican.bmp" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resources\logican.bmp">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="FluentValidation" Version="11.6.0" />
    <PackageReference Include="MahApps.Metro" Version="3.0.0-alpha0457" />
    <PackageReference Include="MaterialDesignColors" Version="3.0.0-ci321" />
    <PackageReference Include="MaterialDesignThemes" Version="5.0.0-ci321" />
    <PackageReference Include="MaterialDesignThemes.MahApps" Version="3.0.0-ci321" />
    <PackageReference Include="Prism.Core" Version="8.1.97" />
  </ItemGroup>

</Project>
